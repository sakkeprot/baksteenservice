          type: string
          description: de classificatie van de halte
          example: REGULIER
        bedieningsTypes:
          enum:
            - '{BUS,TRAM}'
          type: array
          items:
            type: string
          description: 'het type van het voertuig (bus, tram of de combinatie)'
          example: Bus
        plaats:
