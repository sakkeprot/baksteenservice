      required:
        - entiteitnummer
        - haltenummer
        - omschrijving
        - omschrijvingLang
        - districtCode
        - taal
      type: object
      properties:
        entiteitnummer:
          type: string
          description: nummer entiteit (key waarde)
          example: '3'
        haltenummer:
          type: string
          description: halte nummer (key waarde)
          example: '306938'
