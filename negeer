      type: object
      properties:
        entiteitnummer:
          type: string
          description: entiteit nummer (key waarde)
          example: '3'
        lijnnummer:
          type: string
          description: intern lijn nummer (key waarde)
          example: '335'
        lijnnummerPubliek:
          type: string
          description: lijn nummer zoals gekend voor de reizigerr
          example: '335'
        omschrijving:
          type: string
          description: omschrijving van de lijn
          example: Leuven - Aarschot
        vervoerRegioCode:
          type: string
          description: code van de vervoerregio waar de lijn zich bevindt
          example: VR
        publiek:
          type: boolean
          description: is de lijn toegankelijk voor het publiek
          example: true
        vervoertype:
          type: string
          allOf:
            - $ref: '#/components/schemas/Vervoertype'
          description: het vervoerstype van de lijn
          example: METRO
        bedieningtype:
          type: string
          allOf:
            - $ref: '#/components/schemas/Bedieningtype'
          description: het bedieningtype van de lijn
          example: BELBUS
        vervoerslaag:
          type: string
          description: informatie over de vervoerslaag van de lijn
          example: 'Kernnet, Aanvullend net, Flex, Vervoer op Maat'
        lijnGeldigVan:
          type: string
          description: de start datum van de dienstregeling zoals die momenteel geldt
          format: date
          example: '2016-08-31'
        lijnGeldigTot:
          type: string
          description: de eind datum van de dienstregeling zoals die momenteel geldt
          format: date
          example: '2016-10-30'
        links:
          type: array
          items:
            $ref: '#/components/schemas/Link'
    LijnKleurCode:
      type: object
      properties:
        code:
          type: string
        links:
          type: array
          items:
            $ref: '#/components/schemas/Link'
    LijnLijnkleurCodes:
      required:
        - lijn
        - lijnkleurCodes
      type: object
      properties:
        lijn:
          type: object
          allOf:
            - $ref: '#/components/schemas/Lijn'
          description: geeft de sleutel van de lijn
        lijnkleurCodes:
          type: object
          allOf:
            - $ref: '#/components/schemas/LijnkleurCodes'
