  /omleidingen:
    get:
      tags:
        - omleidingen
      summary: /omleidingen - GET
      description: geef alle omleidingen voor een periode van hoogstens een maand
      operationId: get-omleidingen
      parameters:
        - name: eindDatum
          in: query
          description: tijdstip in formaat yyyy-MM-dd'T'HH:mm:ss als eind moment voor de opzoeking van de omleidingen  met als default waarde de huidige tijdstip
          schema:
            $ref: '#/components/schemas/DateTimeParameter'
        - name: startDatum
          in: query
          description: tijdstip in formaat yyyy-MM-dd'T'HH:mm:ss als start moment voor de opzoeking van de omleidingen met als default waarde de huidige tijdstip
          schema:
            $ref: '#/components/schemas/DateTimeParameter'
      responses:
        '200':
          description: request met succes afgehandeld
