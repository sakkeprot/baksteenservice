          in: path
          description: nummer van de entiteit
          required: true
          schema:
            type: integer
            format: int32
        - name: lijnnummer
          in: path
          description: nummer van de lijn
          required: true
          schema:
            type: integer
            format: int32
        - name: richting
          in: path
          description: richting van de lijn
          required: true
          schema:
            $ref: '#/components/schemas/Richting'
        - name: geldigOp
          in: query
          description: tijdstip in formaat yyyy-MM-dd'T'HH:mm:ss met als default waarde de huidige datum waarop de lijnrichting geldig dient te zijn
          schema:
            $ref: '#/components/schemas/CalendarParameter'
      responses:
        '200':
          description: request met succes afgehandeld
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/Haltes'
              example:
                haltes:
                  - entiteitnummer: '3'
                    haltenummer: '306938'
                    omschrijving: Butsel Kerk
                    omschrijvingLang: Boutersem Butsel Kerk
                    gemeentenummer: 57
                    omschrijvingGemeente: Leuven
                    districtCode: GRI
                    geoCoordinaat:
                      latitude: 0
                      longitude: 0
                    halteToegankelijkheden:
                      - MOTORISCHE_BEPERKING
                    hoofdHalte: true
                    taal: NEDERLANDS
                    classificatie: REGULIER
                    bedieningsTypes: Bus
                    plaats:
                      plaatsNaam: Mechelen Veemarkt
                      verkortePlaatsNaam: Veemarkt
                      geoCoordinaat:
                        latitude: 0
                        longitude: 0
                    links:
                      - rel: string
                        url: string
                links:
                  - rel: string
                    url: string
        '500':
          description: service of achterliggende systemen niet beschikbaar
